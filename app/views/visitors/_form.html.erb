<!--<div class="content">-->
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading" style="text-align: center">
        Visitor
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-6">
            <!--<form role="form">-->
  <%= form_for @visitor, :html=> {:id => 'form1'} do |f| %>
      <% if @visitor.errors.any? %>
          <div id="error_explanation">
            <h2><%= pluralize(@visitor.errors.count, "error") %> prohibited this visitor from being saved:</h2>

            <ul>
              <% @visitor.errors.full_messages.each do |message| %>
                  <li><%= message %></li>
              <% end %>
            </ul>
          </div>
      <% end %>
      <div class="field">
        <%= f.label :last_name %><br>
        <%= f.text_field :last_name, class: "form-control" %>
      </div>
      <div class="field">
        <%= f.label :first_name %><br>
        <%= f.text_field :first_name, class: "form-control" %>
      </div>
      <div class="field">
        <%= f.label :group_id %><br>
        <%= f.number_field :group_id, class: "form-control" %>
      </div>
      <div class="field">
        <%= f.label :email %><br>
        <%= f.text_field :email, class: "form-control" %>
      </div>
      <div class="field">
        <%= f.label :contact %><br>
        <%= f.check_box :contact %>
      </div>
      <div class="field">
        <%= f.label :zip_code %><br>
        <%= f.text_field :zip_code, class: "form-control" %>
      </div>
      <div class="field">
        <%= f.label :country_id %><br>
        <%= f.collection_select :country_id, Country.all, :id, :name, {:selected => '215'}, class: "form-control" %>
      </div>
      <div class="actions">
        <%= f.submit %>
      </div>
  <% end %>

  <!--<!%= link_to "Submit", "#", :class => 'button_submit btn btn-danger' %>-->

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
